defaultProxyProvider: default
docker:
  local:
    host: unix:///var/run/docker.sock
    targetHostname: 172.25.0.1
    defaultProxyProvider: default

tailscale:
  providers:
    default:
      authKeyFile: "/config/authkey"
      controlUrl: "https://controlplane.tailscale.com"
  dataDir: /data/

http:
  hostname: 0.0.0.0
  port: 8080

log:
  level: debug
  json: false

proxyAccessLog: true
