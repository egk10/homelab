# Environment variables for docker-compose
# Copy this to .env and set secure values

# Database credentials
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_USER=immich
POSTGRES_DB=immich

MYSQL_PASSWORD=your_secure_mysql_password
MYSQL_ROOT_PASSWORD=your_secure_mysql_root_password

# Redis
REDIS_PASSWORD=your_secure_redis_password

# Nextcloud admin
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=your_secure_nextcloud_password
NEXTCLOUD_DOMAIN=nextcloud.your-tailscale-domain.ts.net

# Vaultwarden
VAULTWARDEN_DOMAIN=https://vaultwarden.your-tailscale-domain.ts.net
ADMIN_TOKEN=your_argon2_hashed_admin_token

# Tailscale
TAILSCALE_AUTH_KEY=tskey-auth-your-tailscale-auth-key

# Ceph RGW S3 credentials (for Immich object storage)
CEPH_S3_ACCESS_KEY_ID=admin
CEPH_S3_SECRET_ACCESS_KEY=your_secure_s3_secret
CEPH_S3_ENDPOINT=http://rgw-host:80
CEPH_S3_BUCKET=immich
CEPH_S3_REGION=us-east-1

# Restic backup credentials (separate user recommended)
BACKUP_S3_ACCESS_KEY_ID=backup_user
BACKUP_S3_SECRET_ACCESS_KEY=your_backup_secret

# System
TZ=America/Toronto
IMMICH_VERSION=release
IMMICH_HOST=0.0.0.0
IMMICH_PORT=2283
